{
  "name": "gastropod",
  "version": "0.0.0",
  "description": "Simple patternlab workflow",
  "main": "index.js",
  "bin": {
    "gastropod": "./bin/gastropod.js"
  },
  "scripts": {
    "test": "mocha -R spec",
    "commit": "git add . && git commit -am 'version bump'",
    "merge-to-master": "git checkout master && git merge develop",
    "merge-to-develop": "git checkout develop && git merge master",
    "deploy": "git push && git push --tags",
    "preversion": "npm run commit && npm run merge-to-master",
    "postversion": "npm run merge-to-develop && npm run deploy"
  },
  "keywords": [
    "gulp",
    "patternlab"
  ],
  "author": "Zenobius Jiricek",
  "license": "MIT",
  "dependencies": {
    "browser-sync": "^2.9.6",
    "chalk": "^1.1.1",
    "coffee-script": "^1.10.0",
    "commander": "^2.8.1",
    "debug": "^2.2.0",
    "gulp-front-matter": "^1.2.3",
    "nconf": "^0.8.0",
    "require-all": "^1.1.0",
    "shimbro": "0.0.11"
  },
  "browserify": {
    "extensions": [
      ".coffee",
      ".html"
    ],
    "transform": [
      "coffeeify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {},
  "browser": {},
  "napa": {},
  "devDependencies": {
    "browser-request": "^0.3.3",
    "browser-sync": "^2.7.13",
    "browserify": "^11.0.1",
    "browserify-ng-html2js": "^1.1.2",
    "browserify-shim": "^3.8.10",
    "browsernizr": "^1.0.2",
    "coffee-script": "^1.9.3",
    "coffeeify": "^1.1.0",
    "del": "^1.2.0",
    "event-stream": "^3.3.1",
    "globby": "^2.1.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-clean": "^0.3.1",
    "gulp-cli": "^0.3.0",
    "gulp-data": "^1.2.0",
    "gulp-dev": "^0.3.0",
    "gulp-extname": "^0.2.0",
    "gulp-front-matter": "^1.2.3",
    "gulp-htmlmin": "^1.1.3",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.3.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-map": "0.0.2",
    "gulp-minify": "0.0.5",
    "gulp-ng-templates": "0.0.6",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-rev-all": "^0.8.21",
    "gulp-sass": "1.3.x",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-swig": "^0.7.4",
    "gulp-tap": "^0.1.3",
    "gulp-util": "^3.0.6",
    "lodash": "^3.10.0",
    "map-stream": "0.0.5",
    "napa": "^1.2.0",
    "nconf": "^0.7.1",
    "run-sequence": "^1.1.1",
    "shimbro": "0.0.11",
    "slug": "^0.9.1",
    "swig": "^1.4.2",
    "through-pipes": "0.0.4",
